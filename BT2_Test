package BT2_LAB7;

import java.util.Scanner;

public class main {
    public static void main(String[] args) {
        MoveablePoint a = new MoveablePoint(1, 2, 10, 50);
        MoveableCircle circle = new MoveableCircle(5, a);
        circle.moveDown();
        circle.moveUp();
        circle.moveLeft();
        circle.moveRight();

    }
}
